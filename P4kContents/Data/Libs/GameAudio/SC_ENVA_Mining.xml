<?xml version="1.0" encoding="utf-16"?>
<ATLConfig atl_name="SC_ENVA_Mining">
  <AudioTriggers>
    <ATLTrigger atl_name="Play_STAM_Fragile_Metal_Tones">
      <WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_STAM_Fragile_Metal_Tones" wwise_radius_max="10" />
      <RTPCDependees>
        <ATLRTPCDependee atl_name="Listener_Atmospheric_Pressure" />
        <ATLRTPCDependee atl_name="Volume" />
        <ATLRTPCDependee atl_name="High_Pass_Filter" />
        <ATLRTPCDependee atl_name="Low_Pass_Filter" />
        <ATLRTPCDependee atl_name="MIX_SFX" />
        <ATLRTPCDependee atl_name="Pitch" />
        <ATLRTPCDependee atl_name="Auxiliary_Send_Volume" />
        <ATLRTPCDependee atl_name="area_fade_distance" />
        <ATLRTPCDependee atl_name="Listener_Volume" />
      </RTPCDependees>
      <ATLPreload atl_name="SC_ENVA_Mining" />
    </ATLTrigger>
  </AudioTriggers>
  <AudioPreloads>
    <ATLPreloadRequest atl_name="SC_ENVA_Mining" atl_type="empty" path="Banks/">
      <ATLPlatforms>
        <Platform atl_config_group_name="default" atl_name="pc" />
      </ATLPlatforms>
      <ATLConfigGroup atl_name="default">
        <WwiseFile wwise_name="ENVA_Mining.bnk" />
      </ATLConfigGroup>
    </ATLPreloadRequest>
  </AudioPreloads>
</ATLConfig>