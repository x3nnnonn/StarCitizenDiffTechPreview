<?xml version="1.0" encoding="utf-16"?>
<ATLConfig atl_name="SC_ENVP_Plant_Globe">
  <AudioTriggers>
    <ATLTrigger atl_name="Play_MCFL_Space_Plant_Globe_Ambience">
      <WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_MCFL_Space_Plant_Globe_Ambience" wwise_radius_max="25" />
      <RTPCDependees>
        <ATLRTPCDependee atl_name="Listener_Atmospheric_Pressure" />
        <ATLRTPCDependee atl_name="Listener_Cone" />
        <ATLRTPCDependee atl_name="MIX_SFX" />
        <ATLRTPCDependee atl_name="Listener_Volume" />
      </RTPCDependees>
      <ATLPreload atl_name="SC_ENVP_Plant_Globe" />
    </ATLTrigger>
    <ATLTrigger atl_name="Stop_MCFL_Space_Plant_Globe_Ambience">
      <WwiseEvent wwise_duration_type="OneShot" wwise_name="Stop_MCFL_Space_Plant_Globe_Ambience" wwise_radius_max="0" />
      <RTPCDependees>
        <ATLRTPCDependee atl_name="Listener_Atmospheric_Pressure" />
        <ATLRTPCDependee atl_name="Listener_Cone" />
        <ATLRTPCDependee atl_name="MIX_SFX" />
        <ATLRTPCDependee atl_name="Listener_Volume" />
      </RTPCDependees>
      <ATLPreload atl_name="SC_ENVP_Plant_Globe" />
    </ATLTrigger>
  </AudioTriggers>
  <AudioPreloads>
    <ATLPreloadRequest atl_name="SC_ENVP_Plant_Globe" atl_type="empty" path="Banks/">
      <ATLPlatforms>
        <Platform atl_config_group_name="default" atl_name="pc" />
      </ATLPlatforms>
      <ATLConfigGroup atl_name="default">
        <WwiseFile wwise_name="ENVP_Plant_Globe.bnk" />
      </ATLConfigGroup>
    </ATLPreloadRequest>
  </AudioPreloads>
</ATLConfig>