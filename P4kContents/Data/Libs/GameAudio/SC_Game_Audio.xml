<?xml version="1.0" encoding="utf-8"?>
<ATLConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" atl_name="SC_Game_Audio">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_Bass_Management_Sine_40hz">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_Bass_Management_Sine_40hz" />
			<RTPCDependees>
				<ATLRTPCDependee atl_name="Obstruction_Amount" />
				<ATLRTPCDependee atl_name="Occlusion_Amount" />
				<ATLRTPCDependee atl_name="MIX_SFX" />
				<ATLRTPCDependee atl_name="pressure_physical" />
				<ATLRTPCDependee atl_name="Listener_Volume" />
			</RTPCDependees>
			<ATLPreload atl_name="SC_Game_Audio" />
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_Game_Audio" atl_type="" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc" />
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="Game_Audio.bnk" />
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>