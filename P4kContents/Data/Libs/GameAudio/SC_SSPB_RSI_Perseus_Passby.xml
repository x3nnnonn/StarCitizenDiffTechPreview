<?xml version="1.0" encoding="utf-16"?>
<ATLConfig atl_name="SC_SSPB_RSI_Perseus_Passby">
  <AudioTriggers>
    <ATLTrigger atl_name="Play_SSPB_RSI_Perseus_NLPC_LP">
      <WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_SSPB_RSI_Perseus_NLPC_LP" wwise_radius_max="3000" />
      <RTPCDependees>
        <ATLRTPCDependee atl_name="SC_Local_Perspective" />
        <ATLRTPCDependee atl_name="MIX_SFX" />
        <ATLRTPCDependee atl_name="Listener_Volume" />
      </RTPCDependees>
      <ATLPreload atl_name="SC_SSPB_RSI_Perseus_Passby" />
    </ATLTrigger>
    <ATLTrigger atl_name="Stop_SSPB_RSI_Perseus_NLPC_LP">
      <WwiseEvent wwise_duration_type="OneShot" wwise_name="Stop_SSPB_RSI_Perseus_NLPC_LP" wwise_radius_max="0" />
      <RTPCDependees>
        <ATLRTPCDependee atl_name="SC_Local_Perspective" />
        <ATLRTPCDependee atl_name="MIX_SFX" />
        <ATLRTPCDependee atl_name="Listener_Volume" />
      </RTPCDependees>
      <ATLPreload atl_name="SC_SSPB_RSI_Perseus_Passby" />
    </ATLTrigger>
  </AudioTriggers>
  <AudioPreloads>
    <ATLPreloadRequest atl_name="SC_SSPB_RSI_Perseus_Passby" atl_type="empty" path="Banks/">
      <ATLPlatforms>
        <Platform atl_config_group_name="default" atl_name="pc" />
      </ATLPlatforms>
      <ATLConfigGroup atl_name="default">
        <WwiseFile wwise_name="SSPB_RSI_Perseus_Passby.bnk" />
      </ATLConfigGroup>
    </ATLPreloadRequest>
  </AudioPreloads>
</ATLConfig>